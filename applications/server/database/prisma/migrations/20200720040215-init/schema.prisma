datasource db {
  provider = "postgresql"
  url = "***"
}

generator client {
  provider = "prisma-client-js"
}

enum Role {
  ADMIN
  MODERATOR
  USER
  STREAMER
}

model User {
  id        String   @id @default(uuid())
  role      Role     @default(USER)
  createdAt DateTime @default(now()) @map(name: "created_at")
  email     String   @unique
  name      String
  password  String


  @@map(name: "users")
}
